<script>
	// Create a copy of the original UserTripList
	var originalUserTripList = [...Model.UserTripList];

	function showPanel(panelId) {
		// Hide all panels
		document.querySelectorAll('.search_panel').forEach(panel => {
			panel.classList.remove('active');
		});

		// Show the selected panel
		document.getElementById(panelId).classList.add('active');

		if (panelId === 'panel2') {
			// Iterate through table rows to find the initial and final destinations
			var tableRows = document.querySelectorAll('#panel2 .table tbody tr');
			var selectedInitDestination, selectedFinalDestination;

			tableRows.forEach(row => {
				selectedInitDestination = row.querySelector('td:nth-child(2)').textContent.trim();
				selectedFinalDestination = row.querySelector('td:nth-child(3)').textContent.trim();
			});

			// If the selected panel is the Trip panel, filter the UserTripList based on selected destinations
			Model.UserTripList = originalUserTripList.filter(trip => {
				return trip.InitDestination === selectedInitDestination && trip.FinalDestination === selectedFinalDestination;
			});

			// Update the table with the filtered UserTripList
			updateTripTable();
		}
	}

	function updateTripTable() {
		var tableBody = document.querySelector('#panel2 .table tbody');
		// Clear existing rows from the table
		tableBody.innerHTML = '';

		// Populate the table with the filtered UserTripList
		Model.UserTripList.forEach(trip => {
			var row = document.createElement('tr');
			row.innerHTML = `
						<td>${trip.VehicleName}</td>
						<td>${trip.InitDestination}</td>
						<td>${trip.FinalDestination}</td>
						<td>${trip.Price}</td>
						<td>${trip.TripDate}</td>
					`;
			tableBody.appendChild(row);
		});
	}
</script>





